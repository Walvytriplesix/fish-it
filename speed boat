-- Ambil reference module asli
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local BoatsHandlingData = require(ReplicatedStorage.Shared.BoatsHandlingData)

-- Ubah semua Speed jadi 1000
for boatName, boatData in pairs(BoatsHandlingData) do
    boatData.Speed = 1000
end

-- Overwrite module (hanya jika game membaca reference module langsung)
for boatName, boatData in pairs(BoatsHandlingData) do
    ReplicatedStorage.Shared.BoatsHandlingData[boatName].Speed = 1000
end

print("Semua boat Speed sekarang 1000!")
